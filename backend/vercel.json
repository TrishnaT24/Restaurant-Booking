{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Content-Security-Policy",
                    "value": "default-src 'none'; script-src 'self' https://vercel.live https://restaurant-booking-frontend-six.vercel.app; style-src 'self' https://restaurant-booking-frontend-six.vercel.app; img-src 'self' https://restaurant-booking-frontend-six.vercel.app;"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://restaurant-booking-frontend-six.vercel.app"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, POST, PUT, DELETE, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Content-Type, Authorization"
                }
            ]
        }
    ]
}
